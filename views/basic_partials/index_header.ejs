<header class="s-header header">
        <div class="header__logo">
            <a class="logo" href="/wordsmith/index.html">
                <img src="/wordsmith/images/logo.svg" alt="Homepage">
            </a>
        </div> <!-- end header__logo -->

        <a class="header__search-trigger" href="/wordsmith/#0"></a>
        <div class="header__search">
            <form role="search" method="get" class="header__search-form" action="#">
                <label>
                    <span class="hide-content">Search for:</span>
                    <input type="search" class="search-field" placeholder="Type Keywords" value="" name="s" title="Search for:" autocomplete="off">
                </label>
                <input type="submit" class="search-submit" value="Search">
            </form>

            <a href="/wordsmith/#0" title="Close Search" class="header__overlay-close">Close</a>

        </div>  <!-- end header__search -->

        <a class="header__toggle-menu" href="/wordsmith/#0" title="Menu"><span>Menu</span></a>
        <nav class="header__nav-wrap">

            <h2 class="header__nav-heading h6">Navigate to</h2>

            <ul class="header__nav">

                <% for (var i = 0; i < frm_data.length; i++) { %>

                <li class="has-children"><a href="/<%= frm_data[i].title.replace(/\s+/g, '').toLowerCase() %>" title=""><%= frm_data[i].title %></a>
                    <ul class="sub-menu">
                        <% for (var j = 0; j < frm_data[i].tsubmenu.length; j++) { %>
                            <li><a href="/<%= frm_data[i].title.replace(/\s+/g, '').toLowerCase() %>/<%= frm_data[i].tsubmenu[j].replace(/\s+/g, '').toLowerCase() %>"><%= frm_data[i].tsubmenu[j] %></a></li>
                        <% }%> 
                    </ul>
                </li>
                <% }%> 
            </ul>
                
            </ul> <!-- end header__nav -->

            <a href="/wordsmith/#0" title="Close Menu" class="header__overlay-close close-mobile-menu">Close</a>

        </nav> <!-- end header__nav-wrap -->

    </header> <!-- s-header -->